  <script>
  //<![CDATA[
var popoutSnippet = {
    snippet: document.getElementById('popout-video-snippet'),
    message: document.getElementById('popout-video-message'),
    video: document.getElementById('popout-video'),
    link: document.getElementById('popout-video-link'),
    play: false,
    showVideo: function() {
        this.message.className = 'hidden';
    }
};

popoutSnippet.message.addEventListener('transitionend', function(e) {
    if (this.className.indexOf('hidden') !== -1) {
        var video = popoutSnippet.video,
            link = popoutSnippet.link;

        this.style.display = 'none';
        link.style.display = 'block';

        link.className = 'show';
        video.className = 'show';

        video.play();
    }
}, false);

popoutSnippet.video.addEventListener('click', function(e) {
    e.stopPropagation();
}, false);

popoutSnippet.snippet.addEventListener('show_snippet', function(e) {
    var snippets = document.getElementById("snippets");

    snippets.addEventListener("click", function(aEvent) {
        if (aEvent.target.nodeName != "a") {
            popoutSnippet.showVideo();
            e.stopPropagation();
        }
    }, false);
}, false);
  //]]>
  </script>
