		<script>
			var tidClicked = false
			var wordsClicked = false
			function clearIn(where){
				if (where == 'tid' && !tidClicked){
					document.forms['search_form'].tid.value = ""
					document.forms['search_form'].tid.style.color = "black"
					tidClicked = true
				}
				if (where == 'search_words' && !wordsClicked){
					document.forms['search_form'].search_words.value = ""
					document.forms['search_form'].search_words.style.color = "black"
					wordsClicked = true
				}
			}

			function checkType(type){
				val = type.value

				if (val == 0 || val == 1 || val == 3 || val == 9)
					document.search_form.cat.disabled = false
				else
					document.search_form.cat.disabled = true

				if (val == 0 || val == 1 || val == 3 || val == 10 || val == 9)
					document.search_form.search_words.disabled = false
				else
					document.search_form.search_words.disabled = true
			}
		</script>
