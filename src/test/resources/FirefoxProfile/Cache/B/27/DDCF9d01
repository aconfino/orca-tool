    <script type="text/javascript" >

        function checkCookies() {
            if (!isEnabledCookies()){
                window.alert('Cookies are not enabled. You must enable cookies before you can start chat');
                return false;
            }
            return true;
        }

        function checkInputParams() {
            return checkCookies();
        }
    </script>
