	<script type="text/javascript">
/* <![CDATA[ */
    (function() {
		// http only (no TTO)
		if(window.location.protocol == 'http:') {
			// no referrer OR a referrer that isnt turbotax
			if(document.referrer == '' || (document.referrer.indexOf('turbotax.intuit.com') == -1 && document.referrer.indexOf('www.turbotax.com') == -1)) {
				// no pages in commerce and microsite
				if(window.location.href.indexOf('/commerce/') == -1 && window.location.href.indexOf('/microsite') == -1) {
					var head= document.getElementsByTagName('head')[0];
					var script= document.createElement('script');
					script.type= 'text/javascript';
					script.src= '/js/survey_util.js?version=12292011';
					head.appendChild(script);
				}
			}
		}
    })();
/* ]]> */
</script>
