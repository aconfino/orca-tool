<!-- DoubleClick -->
	<script type="text/javascript">
		function getDoubleClickHtml() {
			var axel = Math.random() + "";
			var a = axel * 10000000000000;
			var productId = getURLParam("productid");
			if(!productId) {
				productId = 0;
			}
			var u12 = productId + ",0,0.00";
			return('<iframe src="https://fls.doubleclick.net/activityi;src=1984865;type=ttond286;cat=ttost163;u12=' + u12 + ';ord=1;num='+ a + '?" width="1" height="1" frameborder="0"></iframe>');
		}

		function appendDoubleClick() {
			$('body').append(getDoubleClickHtml());
		}

		$(function() {
			setTimeout('appendDoubleClick()', 1000);
		});
	</script>
	
	
