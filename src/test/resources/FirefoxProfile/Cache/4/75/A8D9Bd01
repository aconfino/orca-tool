		function ShowPollResults(showAll){
			win = window.open(
				showAll ? '/poll-results.php' : '/poll.php?show_results&id=18',
				'poll',
				'width=300, height=200, resizable=yes, scrollbars=no, location=no,  toolbar=no, menubar=no, status=no'
			);
