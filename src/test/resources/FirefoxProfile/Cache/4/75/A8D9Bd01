					<SCRIPT>
	function SubmitForm(formName, val){
		var form = document.forms[formName + val];
		subm = false;
		if (val == 1){
			if (form.elements.author.selectedIndex > 0)
				subm = true;
			if (form.elements.cat.selectedIndex > 0)
				subm = true;
			if (form.elements.search_words.value != '')
				subm = true;
			if (form.elements.from_.value != '')
				subm = true;
			if (form.elements.to.value != '')
				subm = true;
		}else{
			if (form.elements.tid.value != '')
				subm = true;
		}
	
		if (subm){
			form.submit();
		}
	}
</SCRIPT>

