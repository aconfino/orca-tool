<script type="text/javascript">

 
 $(function(){
		
	var prodName = "Deluxe";
	if (prodName == "" || prodName == null){
		prodName = "null";
	}
		
	$(".lc-free").click(function(){
		var thisRef = "https://www.lendingclub.com/landing/partner.action?partnerID=75250";
		var linkName = "lcborrower:" + prodName;
		prop14 = linkName + " - "+ s.pageName;
		Tracking.sendClickEvent(prop14);
		window.open(thisRef, '_blank');
	});
	
	$(".lc-paid").click(function(){
		var thisRef = "https://www.lendingclub.com/landing/partner.action?partnerID=75247";
		var linkName = "lcinvestor:"+prodName;
		prop14 = linkName + " - "+ s.pageName;
		Tracking.sendClickEvent(prop14);
		window.open(thisRef, '_blank');
	});
	
});

function sendCalEvent(calEvent)
{
	sendClickEvent(calEvent);
}

</script>
