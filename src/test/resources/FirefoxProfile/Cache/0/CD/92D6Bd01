	<script>
		function checkType(type){
			if (typeof type == 'undefined')
				return
			val = type.value

			if (val == 0 || val == 1 || val == 3 || val == 9)
				document.sf1.cat.disabled = false
			else
				document.sf1.cat.disabled = true
			
			if (val == 0 || val == 1 || val == 3 || val == 10 || val == 9)
				document.sf1.search_words.disabled = false
			else
				document.sf1.search_words.disabled = true
		}
	</script>
