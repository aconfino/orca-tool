			<script>
				function setStat(sid, expire){
					document.cookie = "track=" + sid + "; expires=" + expire
				}
				
				function initChat(cid, sid, login){
					return;
					tcd = document.getElementById('trackCCDiv')
					tcd.innerHTML = "<iframe src='/tracker_popup.php?cid=" + cid + "&sid=" + sid + "&login=" + login + "' width=280 height=140 frameborder=0></iframe>";
				}
				
				function openChat(){
					return;
					if (document.all)
						scr = document.body.scrollTop
					else
						scr = window.scrollY
					tcd = document.getElementById('trackCCDiv')
					tcd.style.width = 280
					tcd.style.height = 140
					tcd.style.left = 300
					tcd.style.top = 200 + scr
					tcd.style.display = 'inline'
				}
				
				function closeCCDiv(reject, cid){
					tcd = document.getElementById('trackCCDiv')
					tcd.style.display = 'none'
					if (reject){
						tcd.innerHTML = "<iframe width=1 height=1 frameborder=0 src='http://web.archive.org/web/20041212193010/http://stat.templatemonster.com//chat_listener.php?cid=" + cid + "&action=ca&type=reject'></iframe>"
					}
				}
			</script>
