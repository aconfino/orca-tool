	function buy(id, exc){
		str = "&PHPSESSID=4eb2d2a773e9441ab943ab4b9a6afaf3";
		//window.open("/cart.php?add=" + id + str, "tmcart", "width=820, height=500, status=yes, scrollbars=yes")
		window.location.href="/cart.php?add="+ id + str;

	}

	function cart(s){
		str = "&PHPSESSID=4eb2d2a773e9441ab943ab4b9a6afaf3";
		//window.open("/cart.php?" + s + str, "tmcart", "width=765, height=420, status=yes, scrollbars=yes")
		//window.open("/cart.php?" + s + str, "tmcart", "width=820, height=500, status=yes, scrollbars=yes")
		window.location.href="/cart.php?"+ s + str;
	}
