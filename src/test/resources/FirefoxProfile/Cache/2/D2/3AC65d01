<script language="javascript" type="text/javascript">
	function buy(id) {
		var url = '/cart.php?add=' + id
						   + '&PHPSESSID=4e65dae57d408bfcd5721b57d19e3349';
		var priceVariantElement = $('input[name=price_variant]:checked');
		var offers = '';
		$('#preview_page_offers li input:checked, input#regular_install_price:checked').each(function() {
			var el = $(this);
			var oid = el.val();
			if ('installation' == oid) {
				oid = el.attr('offer');
			}
			offers += ('' == offers ? '' : '&') + 'offers[]=' + oid;
		});
		if ('undefined' != typeof priceVariantElement && priceVariantElement.length) {
			url = url + "&price_variant="+priceVariantElement.val();
		}
		if ('' != offers) {
			url += '&' + offers;
		}
		window.location.href = url;
	}
	
function cart(s) {
  s = s || '';
  var priceVariantElement = $('input[name=price_variant]:checked');
  var priceStr = '';
  if ('undefined' != typeof priceVariantElement) {
	priceStr = "&price_variant="+priceVariantElement.val();
  }
  if ($('#add-to-cart') && s) {
    $('#button-add-to-cart').hide();
    $('#button-add-to-cart-disabled').show();
    $('#add-to-cart').hide();
    $.ajax({type: "POST",url: "/particles.php",data: "mode=add-to-cart&id="+s+priceStr,success: function(html){$('#add-to-cart').html(html).fadeIn(1000);}
    });
  } else {
      window.location.href = "/cart.php?" + s + priceStr + "&PHPSESSID=4e65dae57d408bfcd5721b57d19e3349";
  }
}

function cartWithYourPrice(id, price) {
  window.location.href = "/cart.php?add="+ id + "&price="+ price
                       + "&PHPSESSID=4e65dae57d408bfcd5721b57d19e3349";
}

    function timeMeasurement()
    {
        
    var d=new Date();
    var __jsEndTime = d.getTime();

        try{
            var execJsTime = __jsEndTime- __jsStartTime;

            if(__jsEndTime > __jsStartTime)
            {
                _gaq.push(['b._trackEvent', 'Time Spent', 'Output Generation', '/index.php', execJsTime]);            } else
            {
                _gaq.push(['b._trackEvent', 'Time Spent', 'Output Generation Error', '/index.php start', __jsStartTime]);                _gaq.push(['b._trackEvent', 'Time Spent', 'Output Generation Error', '/index.php stop', __jsEndTime]);            }
           _gaq.push(['a._trackPageview']);
        } catch(err) {
            //alert(err);
        }
     }
